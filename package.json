{
  "name": "grid-js-component",
  "version": "3.4.1",
  "description": "A reactjs and vuejs grid component.",
  "main": "index.js",
  "scripts": {
    "build": "npm run file2variable && npm run build-ts && npm run build-less && webpack --config demo/webpack.config.js && npm run clean-rev && rev-static --config demo/rev-static.config.js",
    "build-ts": "npm run clean && tsc -p src && tsc -p demo",
    "build-less": "lessc src/grid.less > dist/grid.css && cleancss -o dist/grid.min.css dist/grid.css ./node_modules/perfect-scrollbar/dist/css/perfect-scrollbar.css && lessc demo/common.less > demo/common.css",
    "lint": "tslint \"src/*.ts\" \"src/*.tsx\" \"demo/**/*.ts\" \"demo/**/*.tsx\" && stylelint \"src/*.less\" \"demo/*.less\" --syntax less",
    "clean-rev": "rimraf demo/react.bundle-*.js demo/vue.bundle-*.js",
    "file2variable": "file2variable-cli src/vue-grid.template.html -o src/vue-variables.ts --html-minify",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/grid-js-component.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/grid-js-component/issues"
  },
  "homepage": "https://github.com/plantain-00/grid-js-component#readme",
  "devDependencies": {
    "@types/react": "15.0.23",
    "@types/react-dom": "15.5.0",
    "clean-css-cli": "4.0.12",
    "file2variable-cli": "1.1.0",
    "less": "2.7.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "rev-static": "2.1.2",
    "rimraf": "2.6.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "tslint": "5.1.0",
    "typescript": "2.3.2",
    "vue": "2.3.0",
    "vue-class-component": "5.0.1",
    "webpack": "2.4.1"
  },
  "dependencies": {
    "@types/node": "*",
    "@types/perfect-scrollbar": "*",
    "perfect-scrollbar": "0.6.16",
    "tslib": "1"
  }
}
